<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="ProfileDisplay" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="ProfileCard" id="profile_card">
        <binding name="profile">
          <lookup name="displayed_profile">ProfileDisplay</lookup>
        </binding>
      </object>
    </child>
    <style>
      <class name="card"/>
    </style>
  </template>
</interface>
