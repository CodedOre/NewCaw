[Desktop Entry]
Name=@app_name@
GenericName=Twitter Client
Comment=A window to the social world
# TRANSLATORS: Search terms to find this application, separated by semicolons. The list must end with a semicolon!
Keywords=twitter;
Exec=cawbird %U
Type=Application
Icon=@app_id@
Categories=Network;GTK;
# DBusActivatable=true
StartupNotify=true
X-Purism-FormFactor=Workstation;Mobile;
# Custom uri scheme to receive authentication callbacks
MimeType=x-scheme-handler/cawbird